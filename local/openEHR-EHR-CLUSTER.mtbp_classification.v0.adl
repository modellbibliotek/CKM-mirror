definition
	CLUSTER[at0000] matches {    -- MTBP Classification
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001] occurrences matches {0..1} matches {    -- Variant relevance
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0002,    -- Putative functional relevant
							at0003,    -- Unknown/contradictory functional significane
							at0004]    -- Putative functionally neutral
						}
					}
				}
			}
			allow_archetype CLUSTER[at0005] occurrences matches {0..*} matches {    -- Evidence
				include
					archetype_id/value matches {/.*|openEHR-EHR-CLUSTER\.citation\.v0/}
			}
		}
	}


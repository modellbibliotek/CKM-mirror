	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"juno_labinfo_adapter">
					description = <"Adapter archetype for copying original fields from TakeCare's JUNO API source data since thay need to be available even when thay may cause errors during conversion due to bad or ambiguous incoming data quality (e.g. incoming text where numbers are expected).">
				>
				["at0001"] = <
					text = <"isDeviating">

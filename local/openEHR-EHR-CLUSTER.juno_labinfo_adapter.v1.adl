definition
	CLUSTER[at0000] matches {    -- juno_labinfo_adapter
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0008] occurrences matches {0..1} matches {    -- analysis
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0009] occurrences matches {0..1} matches {    -- analysisId
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0010] occurrences matches {0..1} matches {    -- value
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0003] occurrences matches {0..1} matches {    -- unit
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0007] occurrences matches {0..1} matches {    -- analysisComment
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0002] occurrences matches {0..1} matches {    -- referenceArea
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0001] occurrences matches {0..1} matches {    -- isDeviating
				value matches {
					DV_BOOLEAN matches {*}
				}
			}
			ELEMENT[at0011] occurrences matches {0..1} matches {    -- isAccredited
				value matches {
					DV_BOOLEAN matches {*}
				}
			}
			CLUSTER[at0012] occurrences matches {0..1} matches {    -- PairAnalysis
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0016] occurrences matches {0..1} matches {    -- pairAnalysisSamplingDateTime
						value matches {
							DV_DATE_TIME matches {
								value matches {yyyy-mm-ddTHH:MM:SS}
							}
						}
					}
					allow_archetype CLUSTER[at0015] occurrences matches {0..*} matches {    -- PairAnlyses
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.juno_labinfo_adapter\.v1/}
						exclude
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}


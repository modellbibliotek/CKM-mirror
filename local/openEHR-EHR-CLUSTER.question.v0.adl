				["at0000"] = <
					text = <"Question">
					description = <"Ad hoc question to be used for example in templates as a base e.g. for detailed questions in a questionnaire">
				>
				["at0001"] = <
					text = <"Question">
					description = <"">
					comment = <"The node name \"Question\" should be substituted for the actual question facing the user (e.g. a clinician or patient) and a suitable data type should be used to capture the response (for example an ordinal or coded text with response alternatives or a freetext field).">
				>

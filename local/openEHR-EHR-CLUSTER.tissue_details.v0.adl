definition
	CLUSTER[at0000] occurrences matches {1..*} matches {    -- Tissue details
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0003] occurrences matches {0..1} matches {    -- Tissue
				value matches {
					DV_CODED_TEXT matches {*}
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0001] occurrences matches {0..1} matches {    -- Presence in sample?
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0010,    -- Present
							at0011]    -- Absent
						}
					}
					DV_BOOLEAN matches {*}
				}
			}
			ELEMENT[at0004] occurrences matches {0..1} matches {    -- Normal?
				value matches {
					DV_BOOLEAN matches {*}
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0012,    -- Normal
							at0013,    -- Abnormal
							at0014]    -- Indeterminate
						}
					}
				}
			}
			ELEMENT[at0002] occurrences matches {0..*} matches {    -- Description
				value matches {
					DV_CODED_TEXT matches {*}
					DV_TEXT matches {*}
					DV_BOOLEAN matches {*}
				}
			}
			CLUSTER[at0005] occurrences matches {0..*} matches {    -- Structured description
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0006] occurrences matches {0..1} matches {    -- Description, coded term
						value matches {
							DV_CODED_TEXT matches {*}
						}
					}
					ELEMENT[at0007] occurrences matches {0..1} matches {*}    -- Question & response alternatives
					allow_archetype CLUSTER[at0008] occurrences matches {0..*} matches {    -- Description details
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

